        <externalId>false</externalId>
        <formula>IF(MONTH(Today()) = MONTH( EU_fm_Fecha_Cierre__c ) 
&amp;&amp; EU_fm_Fecha_Cierre__c &gt;= Today() 
&amp;&amp; (ISPICKVAL(EU_001_ls_Estatus__c , &apos;Abierto&apos;) 
|| ISPICKVAL(EU_001_ls_Estatus__c , &apos;Reprogramado&apos;)), 
DAY(DATE( YEAR(TODAY()),IF(MONTH(TODAY()) = 12, 1,MONTH(TODAY()) + 1) , 1)- 1) -DAY( EU_fm_Fecha_Cierre__c )+1, 0)</formula>
