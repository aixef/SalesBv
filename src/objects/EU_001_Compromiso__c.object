    <fields>
        <fullName>EU_001_Fm_Impacto__c</fullName>
        <description>Calculo del impacto en saldo medio para Compromiso</description>
        <externalId>false</externalId>
        <formula>IF(CONTAINS(&apos;Cartera Vigente CP:Cartera Vigente LP:Inversiones:Vista MN:Vista MN:Resto Captación&apos;, TEXT(EU_001_ls_Indicadores_de_cartera__c)) &amp;&amp; (ISPICKVAL(EU_001_ls_Estatus__c , &apos;Abierto&apos;) || ISPICKVAL(EU_001_ls_Estatus__c , &apos;Reprogramado&apos;))&amp;&amp; EU_fm_Fecha_Cierre__c &gt;= Today(), 
((EU_001_dv_Compromiso__c*EU_001_Fm_exito__c*EU_fm_dias__c)/Day(DATE(YEAR(Today()),IF(MONTH(TODAY()) = 12, 1,MONTH(TODAY()) + 1),1)-1))/1000, 
IF(CONTAINS(&apos;INF No Recurrentes:Margen Bruto:INF Recurrentes:Saneamiento:Usuario Nvo de Credito&apos;, TEXT(EU_001_ls_Indicadores_de_cartera__c)) &amp;&amp; (ISPICKVAL(EU_001_ls_Estatus__c , &apos;Abierto&apos;) || ISPICKVAL(EU_001_ls_Estatus__c , &apos;Reprogramado&apos;)) &amp;&amp; EU_fm_Fecha_Cierre__c &gt;= Today(), 
(EU_001_dv_Compromiso__c*EU_001_Fm_exito__c)/1000,
IF(CONTAINS(&apos;Stock Nomina&apos;, TEXT(EU_001_ls_Indicadores_de_cartera__c)) &amp;&amp; (ISPICKVAL(EU_001_ls_Estatus__c , &apos;Abierto&apos;) || ISPICKVAL(EU_001_ls_Estatus__c , &apos;Reprogramado&apos;)) &amp;&amp; EU_fm_Fecha_Cierre__c &gt;= Today(), 
(EU_001_dv_Compromiso__c*EU_001_Fm_exito__c),0)))</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Impacto</label>
        <precision>18</precision>
        <required>false</required>
        <scale>1</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
        <type>Currency</type>
    </fields>
