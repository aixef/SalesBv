    <sharingModel>ControlledByParent</sharingModel>
    <validationRules>
        <fullName>CRM_WF_URL_CTA_es_obligatoria</fullName>
        <active>true</active>
        <description>El URL es obligatorio cuando el check de Call to Action es seleccionado</description>
        <errorConditionFormula>AND(  ISPICKVAL( CRM_WF_Call_Action__c , &quot;Sí&quot;),
        ISBLANK ( CRM_WF_Link_Call_To_Action__c),
            RecordType.Name  = &quot;Landing Pages&quot;
       )</errorConditionFormula>
        <errorDisplayField>CRM_WF_Link_Call_To_Action__c</errorDisplayField>
        <errorMessage>Si seleccionas la casilla CTA debes agregar la URL</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Vigencia_Obligatoria</fullName>
        <active>true</active>
        <description>La fecha de vigencia se vuelve obligatoria cuando el check de Vigencia Always On es falso</description>
        <errorConditionFormula>AND( ISPICKVAL(CRM_WF_Vigencia_Always_On__c , &quot;No&quot;),
         ISBLANK(CRM_WF_Vigencia__c ) ,
            RecordType.Name  = &quot;Landing Pages&quot;
       )</errorConditionFormula>
        <errorDisplayField>CRM_WF_Vigencia__c</errorDisplayField>
        <errorMessage>Si Vigencia Always On no está seleccionado debes especificar la fecha final de vigencia</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Cambios_en_cierre</fullName>
        <active>true</active>
        <errorConditionFormula>AND(  ISPICKVAL( CRM_WF_Estatus__c , &quot;Cierre&quot;) ,
    OR(
        ISCHANGED(  CRM_WF_Nombre_De_La_Version_Arte__c ) ,
        ISCHANGED(  CRM_WF_Nombre_Del_Brief__c ) ,
        ISCHANGED(  CRM_WF_El_Material_Es_Parte__c  ) ,
        ISCHANGED(  CRM_WF_Objetivo_De_La_Version__c ) ,
        ISCHANGED(  GBL_WF_Tipo_Canal_Canal__c ) ,
        ISCHANGED(  GBL_WF_Producto_Suproducto__c ) ,
        ISCHANGED(  CRM_WF_Audiencia__c ) ,
        ISCHANGED(  CRM_WF_Mensaje_Principal__c ) ,
        ISCHANGED(  CRM_WF_Restriccion_O_Condicionantes__c ) ,
        ISCHANGED(  CRM_WF_Copy_Requiere_Datos_Variables__c ) ,
        ISCHANGED(  CRM_WF_Enlista_Los_Datos_Variables__c ) ,
        ISCHANGED(  CRM_WF_Texto_Incluyendo_Datos_Variables__c ) ,
        ISCHANGED(  CRM_WF_Legales__c ) ,
        ISCHANGED(  CRM_WF_Links_Para_Descarga_De_Material__c ) ,
        ISCHANGED(  CRM_WF_Subject__c ) ,
        ISCHANGED(  CRM_WF_Preheader__c ) ,
        ISCHANGED(  CRM_WF_Open_Rate__c ) ,
        ISCHANGED(  CRM_WF_CTOR__c ) ,
        ISCHANGED(  MX_WF_Carpeta_Logos_Fotos__c ) ,
        ISCHANGED(  CRM_WF_El_Material_Es_Parte__c ) ,
        ISCHANGED(  CRM_WF_Objetivo_De_La_Version__c )



)
)</errorConditionFormula>
        <errorMessage>No se pueden modificar un arte aprobado</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Especificar_los_cambios_del_arte</fullName>
        <active>true</active>
        <description>Regla para la aplicación workflow, impide que se haga una nueva versión del arte si el campo historial de cambios está vacío</description>
        <errorConditionFormula>AND ( ISPICKVAL( MX_WF_Crear_Una_Nueva_Version__c , &quot;Sí&quot;) ,
      ISNULL(MX_WF_Historial_de_cambios__c)
     )</errorConditionFormula>
        <errorDisplayField>MX_WF_Historial_de_cambios__c</errorDisplayField>
        <errorMessage>Debes especificar los cambios por los cuales se está creando una nueva versión</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Historial_modificable_solo_cierre</fullName>
        <active>true</active>
        <description>Regla de validación que impide que el campo Historial de cambios se modifique cuando no está en cierre</description>
        <errorConditionFormula>OR(
AND( NOT(ISBLANK(MX_WF_Historial_de_cambios__c))  , 
     ISPICKVAL(CRM_WF_Estatus__c, &quot;Brief Arte&quot;)  ),
AND( ISCHANGED(MX_WF_Historial_de_cambios__c) ,
     NOT(ISPICKVAL( CRM_WF_Estatus__c, &quot;Cierre&quot; ))  )
   )</errorConditionFormula>
        <errorDisplayField>MX_WF_Historial_de_cambios__c</errorDisplayField>
        <errorMessage>El historial de cambios solo se puede editar cuando la etapa se encuentra en cierre</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_LinkArteenCierre</fullName>
        <active>false</active>
        <errorConditionFormula>AND( NOT(ISPICKVAL( CRM_WF_Estatus__c , &quot;Cierre&quot;)), NOT ( ISNULL( MX_WF_URL_Final_Del_Arte__c ) )
)</errorConditionFormula>
        <errorMessage>El link debe ser informado cuando el arte se encuentre en cierre</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Si_Requiere_AB_Testing</fullName>
        <active>true</active>
        <description>Debes indicar Tipo A/B Testing</description>
        <errorConditionFormula>AND 
( 
ISPICKVAL( MX_WF_AB_Testing__c, &quot;Sí&quot;), 
(ISPICKVAL( MX_WF_Tipo_A_B_Testing__c, &quot;&quot;)) 
)</errorConditionFormula>
        <errorMessage>Debes indicar Tipo A/B Testing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Tipo_AB_Testing</fullName>
        <active>true</active>
        <description>No puedes seleccionar Tipo A/B Testing si el canal no requiere A/B Testing</description>
        <errorConditionFormula>AND
(
 ISPICKVAL( MX_WF_AB_Testing__c, &quot;No&quot;),
 NOT(ISPICKVAL( MX_WF_Tipo_A_B_Testing__c, &quot;&quot;))
)</errorConditionFormula>
        <errorMessage>El canal que estás editando no requiere A/B Testing, no debes seleccionar ningún valor para el campo Tipo A/B Testing</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Validacionx100deCTOR</fullName>
        <active>true</active>
        <errorConditionFormula>AND(NOT( ISBLANK(CRM_WF_CTOR__c)),
     OR( CRM_WF_CTOR__c &lt;= 0,  CRM_WF_CTOR__c &gt;100 ) )</errorConditionFormula>
        <errorMessage>El CTOR tiene que ser mayor a 0% y menor o igual a 100%</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Validacionx100deOpenrate</fullName>
        <active>true</active>
        <errorConditionFormula>OR( CRM_WF_Open_Rate__c &lt;= 0, CRM_WF_Open_Rate__c &gt; 100)</errorConditionFormula>
        <errorDisplayField>CRM_WF_Open_Rate__c</errorDisplayField>
        <errorMessage>El open rate tiene que ser mayor a 0% y menor o igual a 100%</errorMessage>
    </validationRules>
