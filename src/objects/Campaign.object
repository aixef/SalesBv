    <validationRules>
        <fullName>CRM_WF_Fecha_Fin_No_Menor_A_Fecha_Inicio</fullName>
        <active>false</active>
        <errorConditionFormula>StartDate  &gt;  EndDate</errorConditionFormula>
        <errorDisplayField>EndDate</errorDisplayField>
        <errorMessage>La fecha de fin no puede ser menor a la fecha de inicio.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WB_ActiveStartDate</fullName>
        <active>false</active>
        <description>Valida si la campaña esta activa, y de estar activa la fecha inicio no puede ser mayor al día de hoy</description>
        <errorConditionFormula>IF( AND(IsActive, StartDate &gt; TODAY(), RecordType.DeveloperName = &apos;MX_WB_Cam_RTG_Tlmktg&apos;), TRUE, FALSE)</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>En una campaña activa la fecha no puede ser mayor al día de hoy</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WB_StartDateNow</fullName>
        <active>false</active>
        <description>Valida que la fecha de inicio sea mayor o igual a hoy</description>
        <errorConditionFormula>IF(OR(AND(ISNEW(), NOT(ISCHANGED(IsActive)), RecordType.DeveloperName = &apos;MX_WB_Cam_RTG_Tlmktg&apos;, (OR(StartDate &lt; Today(),  EndDate &lt; StartDate))), IF(OR(AND(IsActive ,  ISCHANGED(StartDate), (OR(StartDate &lt; Today(),  EndDate &lt; StartDate))), AND(IsActive ,  ISCHANGED(EndDate), (EndDate &lt; StartDate))),true,false)),TRUE,FALSE)</errorConditionFormula>
        <errorDisplayField>StartDate</errorDisplayField>
        <errorMessage>La fecha Inicial no puede ser menor al día actual ni mayor a la fecha fin</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Con_Criterios_es_obligatorio</fullName>
        <active>false</active>
        <description>Los squads distintos a Adquirencia deben de especificar si su campaña el campo Con Criterios</description>
        <errorConditionFormula>AND( ISBLANK ( TEXT( CRM_WF_Con_Criterios__c ) ),
     OR( RecordType.Name = &quot;Squad&quot;, RecordType.Name = &quot;Alianzas Comerciales&quot; ),
       NOT( ISPICKVAL(CRM_WF_Area_Solicitante__c, &quot;SQUAD_ADQUIRENCIA&quot;))
    )</errorConditionFormula>
        <errorDisplayField>CRM_WF_Con_Criterios__c</errorDisplayField>
        <errorMessage>Debe especificar si la campaña requiere Criterios de I.C.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Con_Modelo_es_obligatorio</fullName>
