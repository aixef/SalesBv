        <errorDisplayField>CRM_WF_Con_Modelo__c</errorDisplayField>
        <errorMessage>Debe especificar si la campaña requiere Modelo.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>MX_WF_Criterios_Si</fullName>
        <active>false</active>
        <description>Regla de Validación que se activa para que el usuario especifique qué criterios se necesitan si especificaron que la campaña Sí requiere Criterios.</description>
        <errorConditionFormula>AND( ISPICKVAL( CRM_WF_Con_Criterios__c , &quot;Sí&quot;)  , ISBLANK( CRM_WF_Criterios_De_Ic__c ),  OR(RecordType.DeveloperName = &quot;CRM_WF_Alianzas_Comerciales&quot;,
RecordType.DeveloperName = &quot;CRM_WF_Implementacion&quot;,
RecordType.DeveloperName = &quot;CRM_WF_Campana_Multipromocion&quot;,
RecordType.DeveloperName = &quot;CRM_WF_No_Squad&quot;,
RecordType.DeveloperName = &quot;CRM_WF_Squad&quot;) )</errorConditionFormula>
        <errorDisplayField>CRM_WF_Criterios_De_Ic__c</errorDisplayField>
        <errorMessage>Debes especificar qué criterios son los que se necesitan.</errorMessage>
    </validationRules>
    <validationRules>
