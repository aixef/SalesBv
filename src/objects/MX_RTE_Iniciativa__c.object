       MX_RTE_No_PI_Actual__c  &gt;  MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c , 
      (( MX_RTE_Ano_PI_Actual__c -  MX_RTE_PI_Inicio__r.MX_RTE_Ano_PI__c)*4)+ 
      (MX_RTE_No_PI_Actual__c  - MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c ), 
      ((MX_RTE_Ano_PI_Actual__c -  MX_RTE_PI_Inicio__r.MX_RTE_Ano_PI__c)*4)- 
      (MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c - MX_RTE_No_PI_Actual__c )
   ),
   IF( 
      MX_RTE_PI_Final_Real__r.MX_RTE_No_PI__c   &gt;  MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c , 
      ((MX_RTE_PI_Final_Real__r.MX_RTE_Ano_PI__c  -  MX_RTE_PI_Inicio__r.MX_RTE_Ano_PI__c)*4)+ 
      (MX_RTE_PI_Final_Real__r.MX_RTE_No_PI__c  - MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c )+1, 
      ((MX_RTE_PI_Final_Real__r.MX_RTE_Ano_PI__c -  MX_RTE_PI_Inicio__r.MX_RTE_Ano_PI__c)*4)- 
      (MX_RTE_PI_Inicio__r.MX_RTE_No_PI__c - MX_RTE_PI_Final_Real__r.MX_RTE_No_PI__c )+1  
   ) 
)</formula>
