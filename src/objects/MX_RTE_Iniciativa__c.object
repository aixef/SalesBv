     MX_RTE_PI_Extendido__r.MX_RTE_No_PI__c &gt; MX_RTE_No_PI_Actual__c , 
     ((MX_RTE_PI_Extendido__r.MX_RTE_Ano_PI__c - MX_RTE_Ano_PI_Actual__c )*4)+ 
