        <errorConditionFormula>AND( 
    NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;SMS&quot;) ),
    NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;Push Notification&quot;) ), 
    NOT( CRM_WF_Nombre_Campana__r.CRM_WF_Push_Notification__c ),  
    NOT( CRM_WF_Nombre_Campana__r.CRM_WF_SMS__c ) ,
    RecordType.Name == &quot;Carga SMS Push&quot; 
    )</errorConditionFormula>
        <errorMessage>Este tipo de registro no es valido para los canales seleccionados en la campaña</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Regla_Cancelacion</fullName>
        <active>true</active>
        <description>Para poder cancelar un registro debe de estar previamente aprobado</description>
        <errorConditionFormula>AND( ISPICKVAL (  CRM_WF_Cancelar_Carga__c , &quot;Sí&quot;),  
