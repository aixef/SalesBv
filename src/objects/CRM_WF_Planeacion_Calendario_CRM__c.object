    <validationRules>
        <fullName>CRM_WF_Carga_mayor_a_21_dias</fullName>
        <active>true</active>
        <description>Regla de validación para la aplicación Worfkflow, impide que los usuarios cargen una base con más de tres semanas de anticipación</description>
        <errorConditionFormula>GBL_WF_Dias_Restantes_Carga_Base__c &gt; 21</errorConditionFormula>
        <errorMessage>Solo se pueden planear las cargas con 3 semanas de anticipación</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Justificacion_Obligatoria</fullName>
        <active>true</active>
        <description>El usuario deberá escribir una justificación cuando quiera cancelar el registro</description>
        <errorConditionFormula>AND (   ISPICKVAL( CRM_WF_Cancelar_Carga__c  , &quot;Sí&quot;)  ,  CRM_WF_Justificaci_n_de_Cancelaci_n__c = &quot;&quot;)</errorConditionFormula>
        <errorDisplayField>CRM_WF_Justificaci_n_de_Cancelaci_n__c</errorDisplayField>
        <errorMessage>Debes escribir una justificación de la cancelación</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Multicanal_Obligatorio</fullName>
        <active>true</active>
        <errorConditionFormula>AND(

RecordType.Name == &quot;Carga Multicanal&quot;,
MX_WF_Bcom__c == False,
MX_WF_EECC__c == False,
MX_WF_Ventanilla__c == False,
MX_WF_Podio__c == False,
MX_WF_ATM__c == False,
MX_WF_Bmovil__c == False,
MX_WF_IVR__c == False,
MX_WF_Sdat__c == False
)</errorConditionFormula>
        <errorMessage>Debes seleccionar por lo menos un Canal para la Carga Multicanal</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_No_Hay_Canal_CRM</fullName>
        <active>true</active>
        <description>No se puede elegir el tipo de registro CRM si la campaña no tienen ningún canal de CRM</description>
        <errorConditionFormula>AND(
  
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;ATM&quot;) ) , 
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;Bcom&quot;) ), 
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;Bmovil&quot;) ), 
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;EECC&quot;) ), 
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;IVR&quot;) ), 
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c, &quot;Sdat&quot;) ),
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c , &quot;Ventanilla&quot;) ) ,
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c , &quot;Podio&quot;) ),
     NOT( INCLUDES(CRM_WF_Nombre_Campana__r.CRM_WF_Canales_de_Comunicacion__c , &quot;Bancomer movil&quot;) ),
     RecordType.Name == &quot;Carga Multicanal&quot;
    )</errorConditionFormula>
        <errorMessage>Este tipo de registro no es valido para los canales seleccionados en la campaña</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_No_Hay_Canal_SMS_Push</fullName>
