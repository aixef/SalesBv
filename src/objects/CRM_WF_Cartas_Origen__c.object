    <validationRules>
        <fullName>CRM_WF_Canal_De_Com_Iguales_Prov_Y_Carta</fullName>
        <active>true</active>
        <description>Regla de Validación que no permite guardar un registro de carta Origen si los canales de comunicación entre el proveedor (cuenta) y la Carta Origen son distintos.</description>
        <errorConditionFormula>TEXT (CRM_WF_Canales_Com_Carta_Custodia__c) &lt;&gt;  TEXT ( GBL_WF_Emp_Area_Bancomer__r.GBL_WF_Canales_De_Comunicacion__c  )</errorConditionFormula>
        <errorDisplayField>CRM_WF_Canales_Com_Carta_Custodia__c</errorDisplayField>
        <errorMessage>Los canales de comunicación entre el proveedor (cuenta) y la Carta Origen no pueden ser distintos.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Cartas_R_De_Acuerdo_A_Frecuencia</fullName>
        <active>true</active>
        <description>Regla de validación para que el Total de Cartas recurrentes permitidas este dentro del rango por cada Frecuencia</description>
        <errorConditionFormula>AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Única Vez&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 1 ))  ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Semanal&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 12 ))   ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Quincenal&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 6 ))  ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Mensual&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 3 ))    ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Bimestral&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 2 ))  ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Trimestral&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 1 )) ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Semestral&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 1 ))  ||
AND ( ISPICKVAL ( CRM_WF_Frecuencia__c , &quot;Anual&quot;),( CRM_WF_Total_De_C_Recurrentes__c &gt; 1 ))</errorConditionFormula>
        <errorDisplayField>CRM_WF_Frecuencia__c</errorDisplayField>
        <errorMessage>El Total de Cartas recurrentes no puede ser mayor al rango correspondiente por cada Frecuencia</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Justi_Cancelacion_CO_Obligatoria</fullName>
        <active>true</active>
        <description>Si seleccionas el valor Si en el campo ¿Desea cancelar la Carta Origen? debes agregar una Justificación de Cancelación de Carta</description>
        <errorConditionFormula>AND( 
ISPICKVAL( CRM_WF_Desea_Cancelar_La_Carta_Origen__c , &quot;Si&quot;), 
CRM_WF_Just_De_Cancelacin_Carta__c = &quot;&quot;,
RecordType.Name = &quot;Carta Origen&quot;  || 
RecordType.Name = &quot;Carta Origen Readonly&quot; 
)</errorConditionFormula>
        <errorDisplayField>CRM_WF_Just_De_Cancelacin_Carta__c</errorDisplayField>
        <errorMessage>Debes agregar una Justificación de Cancelación para la Carta Origen</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_No_De_Bases_Solicitadas_Mayor_A_1</fullName>
        <active>true</active>
        <errorConditionFormula>CRM_WF_Numero_De_Bases_A_Enviar__c  &lt; 1</errorConditionFormula>
        <errorDisplayField>CRM_WF_Numero_De_Bases_A_Enviar__c</errorDisplayField>
        <errorMessage>El número de bases solicitadas debe ser mayor o igual a 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_No_De_Registros_Mayor_A_1</fullName>
        <active>true</active>
        <errorConditionFormula>CRM_WF_Total_de_Registros__c &lt; 1</errorConditionFormula>
        <errorDisplayField>CRM_WF_Total_de_Registros__c</errorDisplayField>
        <errorMessage>El número total de registros debe ser mayor o igual a 1</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Val_A_Partir_De_En_Rango</fullName>
        <active>false</active>
        <errorConditionFormula>CRM_WF_A_Partir_De__c &lt; 
CRM_WF_Campana_Carta_Custodia__r.StartDate 

|| 

CRM_WF_A_Partir_De__c &gt;
CRM_WF_Campana_Carta_Custodia__r.EndDate</errorConditionFormula>
        <errorDisplayField>CRM_WF_A_Partir_De__c</errorDisplayField>
        <errorMessage>La fecha inicial de la Carta Origen debe estar dentro del rango de la vigencia autorizada en la Campaña</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Validacion_Bases</fullName>
        <active>true</active>
        <errorConditionFormula>CRM_WF_Total_Bases_En_C_Recurrentes__c &gt;  CRM_WF_Numero_De_Bases_A_Enviar__c</errorConditionFormula>
        <errorDisplayField>CRM_WF_Numero_De_Bases_A_Enviar__c</errorDisplayField>
        <errorMessage>El total de bases en Cartas recurrentes no puede ser mayor al total de bases de la campaña</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>CRM_WF_Validacion_Registros</fullName>
        <active>true</active>
        <errorConditionFormula>CRM_WF_Total_Registros_En_C_Recurrentes__c &gt;  CRM_WF_Total_de_Registros__c</errorConditionFormula>
        <errorDisplayField>CRM_WF_Total_de_Registros__c</errorDisplayField>
        <errorMessage>El total de registros en cartas recurrentes no puede ser mayor al total de registros de la campaña</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
