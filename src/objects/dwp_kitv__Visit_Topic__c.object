    <validationRules>
        <fullName>MX_EU_NoPermiteOportunidadesCerradas</fullName>
        <active>true</active>
        <description>Valida que no se puedan ligar oportunidades que estén cerradas durante la creación de un tema, y durante alguna modificación permite que este cerrada pero con fecha de modificación posterior.</description>
        <errorConditionFormula>AND(
                ISNEW(),
                dwp_kitv__opportunity_id__r.IsClosed
)</errorConditionFormula>
        <errorDisplayField>dwp_kitv__opportunity_id__c</errorDisplayField>
        <errorMessage>No se puede seleccionar una oportunidad cerrada</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_kitv__empty_topic</fullName>
