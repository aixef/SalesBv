    <validationRules>
        <fullName>MX_PF_obligaContactoUnico</fullName>
        <active>true</active>
        <description>Permite que solo haya un contacto seleccionado en las visitas de PFLEAN</description>
        <errorConditionFormula>IF(
    AND( ISNEW(),
         dwp_kitv__visit_id__r.dwp_kitv__visit_contact_persons_number__c &gt;= 1,
         OR(dwp_kitv__visit_id__r.RecordType.DeveloperName = &apos;Reporte_de_visita_PF_LEAN_MEDIANA&apos;,
            dwp_kitv__visit_id__r.RecordType.DeveloperName = &apos;Reporte_de_visita_PF_LEAN_Otro_Seg&apos;,
            dwp_kitv__visit_id__r.RecordType.DeveloperName = &apos;Reporte_de_visita_PF_LEAN&apos;
           )
       ),
    true,
    false
   )</errorConditionFormula>
        <errorMessage>La visita solo puede tener un contacto Asignado</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>dwp_kitv__Change_contact</fullName>
        <active>true</active>
        <description>You cannot change the contact.</description>
        <errorConditionFormula>ISCHANGED(  dwp_kitv__contact_id__c )</errorConditionFormula>
        <errorMessage>It is not allowed to change the contact. Please, delete the visit contact.</errorMessage>
    </validationRules>
