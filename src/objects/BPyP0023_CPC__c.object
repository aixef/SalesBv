    <fields>
        <fullName>BPyP_fm_AvanceCPC__c</fullName>
        <externalId>false</externalId>
        <formula>IF(  OR( ISBLANK( BPyP0023_lm_Hobbie__c ) ,   INCLUDES(BPyP0023_lm_Hobbie__c, &apos;Ninguno&apos;) ) , 0, 0.083)+
IF( ISBLANK(TEXT(  BPyP0023_ls_GustaArte__c  )) , 0, 0.083)+
IF(  OR( ISBLANK( BPyP0023_lm_ComidaFavorita__c ) ,   INCLUDES(BPyP0023_lm_ComidaFavorita__c, &apos;Ninguno&apos;) ) , 0, 0.083)+
IF( ISBLANK(TEXT( MX_BPP_Deporte__c )) , 0, 0.083)+
IF(  OR( ISBLANK( BPyP0023_lm_DeporteQueSigue__c ) ,   INCLUDES(BPyP0023_lm_DeporteQueSigue__c, &apos;Ninguno&apos;) ) , 0, 0.083)+
IF(  OR( ISBLANK( MX_BPP_ViajeFrecuente__c) ,   INCLUDES(MX_BPP_ViajeFrecuente__c, &apos;Ninguno&apos;) ) , 0, 0.083)+
IF( ISBLANK( BPyP0023_ls_EventosBBVA__c ) , 0, 0.083)+
IF( ISBLANK( BPyP0023_tx_UniveridadAsistio__c ) , 0, 0.083)+
IF( ISBLANK(TEXT( BPyP0023_tx_LugarComunicacion__c )) , 0, 0.083)+
IF( ISBLANK(TEXT( BPyP0023_ls_EnvioInformacion__c )) , 0, 0.083)+
IF( ISBLANK(TEXT( MX_BPP_Tiene_hijos__c )) , 0, 0.083)+
IF( ISBLANK(TEXT( BPyP0023_ls_CuantosHijosTiene__c )) , 0, 0.083)</formula>
        <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
        <label>Avance CPC</label>
        <precision>18</precision>
        <required>false</required>
        <scale>0</scale>
        <trackHistory>false</trackHistory>
        <trackTrending>false</trackTrending>
