    <formulas>
        <name>varFecha_Solicitud</name>
        <dataType>Date</dataType>
        <expression>IF
(
WEEKDAY(DATEVALUE({!varFechaSolicitudHora})) = 6,
TODAY()+3,
IF
(
WEEKDAY(DATEVALUE({!varFechaSolicitudHora})) = 7,
TODAY()+2,
IF
(
WEEKDAY(DATEVALUE({!varFechaSolicitudHora})) = 1,
TODAY()+1,
IF
(
{!varFechaSolicitudHora}&gt;{!varHoraActual} , 
		TODAY()+1,
		TODAY()
)
)
)
)</expression>
    </formulas>
