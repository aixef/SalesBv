    <formulas>
        <name>varUniversoPromedio</name>
        <dataType>Number</dataType>
        <expression>IF(
   {!varNoArtesPruebaTotales}=0,
   FLOOR({!var_Universo_Muestra}/1),
   FLOOR({!var_Universo_Muestra}/({!varNoArtesPruebaTotales}+1))
)</expression>
        <scale>0</scale>
    </formulas>
