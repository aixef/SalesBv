        <expression>&quot;La distribución del universo muestra es la siguiente:&quot; &amp; {!varSaltoLinea}  &amp; {!varSaltoLinea}  &amp; IF
(
	{!varNoArtesPruebaTotales}=0,
	&quot;Arte control: &quot; &amp; TEXT({!varUniversoPromedio}),
IF
(
	{!varNoArtesPruebaTotales}=1,
	&quot;Arte control: &quot; &amp; TEXT({!varMODUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba B: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  ,
	IF
(
		{!varNoArtesPruebaTotales}=2,
		&quot;Arte control: &quot; &amp; TEXT({!varMODUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba B: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba C: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  ,
	IF
(
	{!varNoArtesPruebaTotales}=3,
			&quot;Arte control: &quot; &amp; TEXT({!varMODUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba B: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba C: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba D: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  ,
	IF
(
	{!varNoArtesPruebaTotales}=4,
				&quot;Arte control: &quot; &amp; TEXT({!varMODUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba B: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba C: &quot;&amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba D: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  &amp;
&quot;Arte de prueba E: &quot; &amp; TEXT({!varUniversoPromedio}) &amp; {!varSaltoLinea}  ,
