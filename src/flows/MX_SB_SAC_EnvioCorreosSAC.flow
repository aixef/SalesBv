    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>NOT(ISBLANK([Case].MX_SB_SAC_CorreoCuenta__c)) &amp;&amp; [Case].MX_SB_SAC_EsCorreoAlterno__c  = FALSE &amp;&amp; 
NOT(ISBLANK([Case].Description)) &amp;&amp; 
ISBLANK([Case].ClosedDate) &amp;&amp; 
[Case].MX_SB_SAC_CorreoEnviado__c =FALSE &amp;&amp;
(
(ISPICKVAL([Case].Reason, &quot;Cambios con afectación de primas&quot;) &amp;&amp;
   (ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Descripción de unidad (descripción/carrocería/modelo/cilindros/puertas/transmisión/ocupantes)&quot;) || 
   ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Estado de circulación&quot;) || 
   ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Excluir cobertura&quot;) || 
   ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Incluir cobertura&quot;) 
)) || 
(ISPICKVAL([Case].Reason, &quot;Cambios sin afectación de primas&quot;) &amp;&amp;
  (ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Cambio de beneficiario preferente&quot;) ||
     (ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Conductor adicional&quot;) &amp;&amp;
      ISPICKVAL([Case].MX_SB_SAC_InformacionAdicional__c, &quot;Modificación de mas de 2 dígitos&quot;)) ||  
    (ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Corregir titular&quot;) &amp;&amp;
      ISPICKVAL([Case].MX_SB_SAC_InformacionAdicional__c, &quot;Modificación de mas de 2 dígitos&quot;)) || 
   ISPICKVAL([Case].MX_SB_SAC_Detalle__c , &quot;Excluir beneficiario preferente&quot;) ||   
