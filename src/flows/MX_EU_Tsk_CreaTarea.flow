        <expression>IF(NOT(ISNULL({!Compromiso.EU_001_Rb_Oportunidad__c})), {!Compromiso.EU_001_Rb_Oportunidad__c}, IF(NOT(ISNULL({!Compromiso.Cliente__c})), {!Compromiso.Cliente__c}, {!Compromiso.EU_001_Rb_Revisi_n_Individual_RI__c}  ))</expression>
    </formulas>
    <formulas>
