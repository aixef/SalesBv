        <name>FechaNotificacion</name>
        <dataType>DateTime</dataType>
        <expression>IF({!Compromiso.EU001_ca_priorizado__c}, DATETIMEVALUE( IF( DATE(YEAR({!Compromiso.EU_fm_Fecha_Cierre__c}), MONTH({!Compromiso.EU_fm_Fecha_Cierre__c}), DAY({!Compromiso.EU_fm_Fecha_Cierre__c})) - DATE(YEAR({!$Flow.CurrentDate}), MONTH({!$Flow.CurrentDate}), DAY({!$Flow.CurrentDate})) &gt;1, DATE( YEAR({!Compromiso.EU_fm_Fecha_Cierre__c}),MONTH({!Compromiso.EU_fm_Fecha_Cierre__c}), DAY({!Compromiso.EU_fm_Fecha_Cierre__c}) )-1, IF( DATE(YEAR({!Compromiso.EU_fm_Fecha_Cierre__c}), MONTH({!Compromiso.EU_fm_Fecha_Cierre__c}), DAY({!Compromiso.EU_fm_Fecha_Cierre__c})) - DATE(YEAR({!$Flow.CurrentDate}), MONTH({!$Flow.CurrentDate}), DAY({!$Flow.CurrentDate})) = 1, DATE(YEAR({!Compromiso.EU_fm_Fecha_Cierre__c}), MONTH({!Compromiso.EU_fm_Fecha_Cierre__c}), DAY({!Compromiso.EU_fm_Fecha_Cierre__c})), DATE(YEAR({!Compromiso.EU_fm_Fecha_Cierre__c}), MONTH({!Compromiso.EU_fm_Fecha_Cierre__c}), DAY({!Compromiso.EU_fm_Fecha_Cierre__c}))+1 )) )-540/1440, null)</expression>
    </formulas>
    <formulas>
