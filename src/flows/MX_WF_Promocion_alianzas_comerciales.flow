        </connector>
        <fields>
            <name>TextoBanco</name>
            <fieldText>&lt;p&gt;&lt;b style=&quot;background-color: rgb(255, 255, 255); color: rgb(62, 62, 60);&quot;&gt;Costo de la Campaña: &lt;/b&gt;&lt;span style=&quot;background-color: rgb(255, 255, 255); color: rgb(62, 62, 60);&quot;&gt;Agregar valores entre 0 y 100 para cada campo. La suma debe ser igual a 100.&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>Banco</name>
            <dataType>Number</dataType>
            <defaultValue>
                <numberValue>0.0</numberValue>
            </defaultValue>
            <fieldText>Banco %  (incluye fondo de marca)</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <scale>0</scale>
            <validationRule>
                <errorMessage>&lt;p&gt;&lt;b&gt;Recuerda que:&lt;/b&gt;&lt;/p&gt;&lt;ol&gt;&lt;li&gt;&lt;b&gt;El porcentaje debe ser un número entre 0 y 100&lt;/b&gt;&lt;/li&gt;&lt;li&gt;&lt;b&gt;La suma de los tres porcentajes debe ser igual a 100&lt;/b&gt;&lt;/li&gt;&lt;/ol&gt;</errorMessage>
                <formulaExpression>AND( {!Banco}&lt;=100, {!Banco}&gt;=0, {!Banco}+{!Comercio}+{!Firma}=100)</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Comercio</name>
            <dataType>Number</dataType>
            <defaultValue>
                <numberValue>0.0</numberValue>
            </defaultValue>
            <fieldText>Comercio %</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <scale>0</scale>
            <validationRule>
                <errorMessage>&lt;p&gt;&lt;b&gt;Recuerda que:&lt;/b&gt;&lt;/p&gt;&lt;ol&gt;&lt;li&gt;&lt;b&gt;El porcentaje debe ser un número entre 0 y 100&lt;/b&gt;&lt;/li&gt;&lt;li&gt;&lt;b&gt;La suma de los tres porcentajes debe ser igual a 100&lt;/b&gt;&lt;/li&gt;&lt;/ol&gt;&lt;p&gt;&lt;br&gt;&lt;/p&gt;</errorMessage>
                <formulaExpression>AND({!Comercio}&lt;=100, {!Comercio}&gt;=0,{!Banco}+{!Comercio}+{!Firma}=100)</formulaExpression>
            </validationRule>
        </fields>
        <fields>
            <name>Firma</name>
            <dataType>Number</dataType>
            <defaultValue>
                <numberValue>0.0</numberValue>
            </defaultValue>
            <fieldText>Firma %</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
            <scale>0</scale>
            <validationRule>
                <errorMessage>&lt;p&gt;&lt;b&gt;Recuerda que:&lt;/b&gt;&lt;/p&gt;&lt;ol&gt;&lt;li&gt;&lt;b&gt;El porcentaje debe ser un número entre 0 y 100&lt;/b&gt;&lt;/li&gt;&lt;li&gt;&lt;b&gt;La suma de los tres porcentajes debe ser igual a 100&lt;/b&gt;&lt;/li&gt;&lt;/ol&gt;</errorMessage>
                <formulaExpression>AND({!Firma}&lt;=100,{!Firma}&gt;=0,{!Banco}+{!Comercio}+{!Firma}=100)</formulaExpression>
            </validationRule>
        </fields>
