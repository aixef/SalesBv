    <rules>
        <fullName>Actualizar Brief a Aprobado</fullName>
        <actions>
            <name>CRM_WF_Etapa_Brief_Cambia_A_Aprobado</name>
            <type>FieldUpdate</type>
        </actions>
        <active>true</active>
        <criteriaItems>
            <field>CRM_WF_Brief__c.CRM_WF_Numero_de_Canales_aprobados__c</field>
            <operation>greaterOrEqual</operation>
            <value>1</value>
        </criteriaItems>
        <description>Regla de flujo de trabajo para cambiar la etapa del brief a aprobado</description>
        <triggerType>onAllChanges</triggerType>
    </rules>
    <rules>
        <fullName>Actualizar Brief a Cancelado</fullName>
